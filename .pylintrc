[MESSAGES CONTROL]
disable =
    missing-docstring,
    invalid-name,
    line-too-long,
    too-few-public-methods,
    too-many-instance-attributes,
    too-many-arguments,
    too-many-locals,
    too-many-statements,
    too-many-lines,
    duplicate-code,
    unnecessary-pass,
    pointless-string-statement,
    unspecified-encoding,
    wrong-import-position,
    wrong-import-order,
    ungrouped-imports,
    global-statement,
    global-variable-not-assigned,
    redefined-outer-name,
    redefined-builtin,
    protected-access,
    super-with-arguments,
    trailing-whitespace,
    multiple-statements,
    multiple-imports,
    wildcard-import,
    unused-import,
    unused-variable,
    unused-argument,
    too-many-nested-blocks
